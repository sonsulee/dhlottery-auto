{
	"name": "dhlottery-auto",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"format": "biome format --write",
		"format:check": "biome format",
		"lint": "biome lint --write",
		"lint:check": "biome lint",
		"check": "biome check --write",
		"check:ci": "biome check",
		"check:unsafe": "biome check --unsafe --write",
		"ci": "pnpm run format:check && pnpm run lint:check && pnpm run build",
		"pre-commit": "pnpm run format:check && pnpm run lint:check",
		"build": "swc src -d dist",
		"dev": "swc src/index.ts -o dist/index.js && node dist/index.js",
		"start": "node dist/src/index.js",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@swc/cli": "^0.7.8",
		"@swc/core": "1.13.5",
		"@types/node": "^24.7.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.4",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"playwright": "^1.56.0"
	},
	"lint-staged": {
		"*.{ts,js}": [
			"biome check --write"
		]
	},
	"packageManager": "yarn@4.10.3"
}
